            - /bin/bash
            - -c
            - |
              set -e
              
              # 检查并安装 wget
              echo "[$(date '+%Y-%m-%d %H:%M:%S')] Installing dependencies..."
              if command -v apt-get &> /dev/null; then
                  # Debian/Ubuntu 系统
                  apt-get update && apt-get install -y wget
              elif command -v apk &> /dev/null; then
                  # Alpine 系统
                  apk add --no-cache wget
              elif command -v yum &> /dev/null; then
                  # CentOS/RHEL 系统
                  yum install -y wget
              else
                  echo "无法确定包管理器类型"
                  exit 1
              fi
              
              # 安装 AzCopy
              echo "[$(date '+%Y-%m-%d %H:%M:%S')] Installing AzCopy..."
              wget -q -O azcopy.tar.gz https://aka.ms/downloadazcopy-v10-linux
              tar -xf azcopy.tar.gz --strip-components=1
              chmod +x azcopy
              mv azcopy /usr/local/bin/
              
              # 验证 AzCopy 安装
              echo "[$(date '+%Y-%m-%d %H:%M:%S')] AzCopy version:"
              /usr/local/bin/azcopy --version
              
              # 设置环境变量
              export PATH=/usr/local/bin:$PATH
              
              # 生成 SAS 令牌 (24小时有效期)
              echo "[$(date '+%Y-%m-%d %H:%M:%S')] Generating SAS tokens..."
              END_TIME=$(date -u -d "24 hours" '+%Y-%m-%dT%H:%M:%SZ')
              
              PRIMARY_SAS=$(az storage account generate-sas \
                --account-name $PRIMARY_STORAGE \
                --account-key "$PRIMARY_KEY" \
                --expiry $END_TIME \
                --permissions rl \
                --resource-types sco \
                --services f \
                -o tsv)
              
              DR_SAS=$(az storage account generate-sas \
                --account-name $DR_STORAGE \
                --account-key "$DR_KEY" \
                --expiry $END_TIME \
                --permissions rwdl \
                --resource-types sco \
                --services f \
                -o tsv)
              
              # 执行同步
              echo "[$(date '+%Y-%m-%d %H:%M:%S')] Starting sync from $PRIMARY_STORAGE to $DR_STORAGE"
              SYNC_START=$(date +%s)
              
              azcopy sync \
                "https://$PRIMARY_STORAGE.file.core.windows.net/$FILE_SHARE?$PRIMARY_SAS" \
                "https://$DR_STORAGE.file.core.windows.net/$FILE_SHARE?$DR_SAS" \
                --recursive \
                --delete-destination=true \
                --compare-hash=MD5 \
                --cap-mbps=0 \
                --log-level=INFO
              
              SYNC_END=$(date +%s)
              SYNC_DURATION=$((SYNC_END - SYNC_START))
              
              echo "[$(date '+%Y-%m-%d %H:%M:%S')] Sync completed in $SYNC_DURATION seconds"
              
              # 输出同步统计
              echo "=== Sync Statistics ==="
              azcopy jobs show --with-status=completed --output-type=json | tail -1 || true
            env:
            - name: PRIMARY_STORAGE
              valueFrom:
                secretKeyRef:
                  name: sync-credentials
                  key: primary-storage
            - name: PRIMARY_KEY
              valueFrom:
                secretKeyRef:
                  name: sync-credentials
                  key: primary-key
            - name: DR_STORAGE
              valueFrom:
                secretKeyRef:
                  name: sync-credentials
                  key: dr-storage
            - name: DR_KEY
              valueFrom:
                secretKeyRef:
                  name: sync-credentials
                  key: dr-key
            - name: FILE_SHARE
              valueFrom:
                secretKeyRef:
                  name: sync-credentials
                  key: file-share
            resources:
              requests:
                memory: "256Mi"
                cpu: "100m"
              limits:
                memory: "512Mi"
                cpu: "500m"
          restartPolicy: OnFailure
EOF



