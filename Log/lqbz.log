apiVersion: batch/v1
kind: CronJob
metadata:
  name: file-sync-cronjob
spec:
  schedule: "*/5 * * * *"  # 每5分钟执行一次
  concurrencyPolicy: Forbid
  successfulJobsHistoryLimit: 3
  failedJobsHistoryLimit: 1
  jobTemplate:
